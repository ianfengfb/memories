<script>
    function myFunction() {
            myVar = setTimeout(showPage, 2200);
        }

        function showPage() {
            document.getElementById("overlay").style.display = "none";
            const element = document.getElementById("mainContent");
            element.classList.remove("content");
        }
</script>
